<MudNavMenu Color="Color.Tertiary">
    <MudText Typo="Typo.h6" Class="px-4">@TitleMenu</MudText>
    <MudNavLink Href="/" Icon="@Icons.Material.Filled.VideogameAsset" IconColor="Color.Inherit" Match="NavLinkMatch.All">Jeux</MudNavLink>
    <MudNavLink Href="/steamGames" Icon="@Icons.Custom.Brands.Steam" IconColor="Color.Inherit" Match="NavLinkMatch.Prefix">Steam</MudNavLink>
    <MudNavLink Href="/playstationGames" Icon="@Icons.Material.Filled.VideogameAsset" IconColor="Color.Inherit" Match="NavLinkMatch.Prefix">Playstation</MudNavLink>
    <MudNavGroup Title="Paramètres" Expanded="false">
        <MudNavLink Href="/categories">Catégories</MudNavLink>
        <MudNavLink Href="/platforms">Platforms</MudNavLink>
        <MudNavLink Href="/series">Séries</MudNavLink>
    </MudNavGroup>
</MudNavMenu>

@code {
    [Parameter] public string TitleMenu { get; set; } = string.Empty;
}